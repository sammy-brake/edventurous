<div class="booking">
<% if @experience %>
<h2> View the dates of your past and currently scheduled trips to <%=@experience.title%>  </h2>
<div id="ajax">
</div>
<%end%> 
</div>
<ul>
<% @bookings.each do |booking| %>
   <% @stringId = booking.id.to_s%>
    <%@idName = "show" + @stringId%>
<div class="dates">
   <%=link_to "#{booking.date.strftime('%b %d, %Y')}: Click for More", href="/field_experiences/#{@experience.id}/bookings/#{booking.id}"%>
   <div id=<%=@idName%>></div>
   </div>
<%end%>
</ul>
</div>

<div class="welcome">
<% if logged_in? %>
<%= link_to 'Home', user_path(current_user), class: 'rootbtn'%>
<%= link_to "All Experiences", field_experiences_path, class: 'rootbtn'%>
<%= link_to 'Log Out', session_path, method: :delete, class: "rootbtn" %>
<% else %>
<%= link_to 'Home', root_url, class: 'rootbtn'%>
<% end %>
</div>


